<!-- GEN_COMPSET_MATCH settings            -->
<!-- ===================================== -->

<compset GEN_COMPSET_MATCH="_CN"
         CLM_CONFIG_OPTS="-bgc cn"
/>

<compset GEN_COMPSET_MATCH="_CAM5"
         CAM_CONFIG_OPTS="-phys cam5" 
         CICE_NAMELIST_OPTS="cam5=.true."  
         CCSM_CCOST="1" 
/>

<compset GEN_COMPSET_MATCH="_MAM3"
         CAM_CONFIG_OPTS="-chem trop_mam3" 
/>

<compset GEN_COMPSET_MATCH="_CHEM"
         CAM_CONFIG_OPTS="-chem super_fast_llnl" 
/>

<compset GEN_COMPSET_MATCH="_WACCM"
         CAM_CONFIG_OPTS="-phys cam4 -chem waccm_mozart_v1" 
         CCSM_CCOST="3" 
/>

<compset GEN_COMPSET_MATCH="_WACCMX"
         CAM_CONFIG_OPTS="-phys cam4 -chem waccm_mozart -waccmx"
         ATM_NCPL="288"
/>

<compset GEN_COMPSET_MATCH="_SD"
         CCSM_CO2_PPMV="367.0"
         CLM_NML_USE_CASE="20thC_transient" 
         CALENDAR="GREGORIAN" 
/>

<compset GEN_COMPSET_MATCH="_MOZART"
         CAM_CONFIG_OPTS="-chem trop_mozart" 
/>

<compset GEN_COMPSET_MATCH="_MOZMAM"
         CAM_CONFIG_OPTS="-phys cam5 -chem trop_mozart_mam3" 
         CCSM_CCOST="3" 
/>

<compset GEN_COMPSET_MATCH="_MOZSOA"
         CAM_CONFIG_OPTS="-chem trop_mozart_soa" 
         CAM_NML_USE_CASE="mozart_megan_emis"
         CCSM_CCOST="3" 
/>

<compset GEN_COMPSET_MATCH="_STRATMAM3"
         CAM_CONFIG_OPTS="-phys cam5 -chem trop_strat_mam3" 
         CAM_NML_USE_CASE="cam5_trop_strat_chem"
         CCSM_CCOST="3" 
/>
<compset GEN_COMPSET_MATCH="_STRATMAM7"
         CAM_CONFIG_OPTS="-phys cam5 -chem trop_strat_mam7" 
         CAM_NML_USE_CASE="cam5_trop_strat_chem"
         CCSM_CCOST="3" 
/>

<compset GEN_COMPSET_MATCH="_BGC-BPRP"
         CAM_CONFIG_OPTS="-co2_cycle" 
/>

<compset GEN_COMPSET_MATCH="_BGC-BDRD"
         CAM_CONFIG_OPTS="-co2_cycle" 
/>

<compset GEN_COMPSET_MATCH="_GLC"
